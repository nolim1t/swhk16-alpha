  </head>

  <body>
    <nav class="navbar navbar-fixed-top navbar-inverse navbar-custom" role="navigation">
    <div class="container-fluid">
      <div class="navbar-header">
        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#myNavbar">
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>                        
        </button>
        <a href= "/cards/index" class="navbar-brand navbar-logo">
          <%= image_tag('logolight.png', width: "50") %>
        </a>
      </div>
      <div class="collapse navbar-collapse" id="myNavbar">
  <!--       <ul class="nav navbar-nav">
          <li><a href="#">Home</a></li>
          <li><a href="#">FAQ</a></li>
          <li><a href="#">Pricing</a></li>
        </ul> -->
          <% if user_signed_in? %>
            <%= render "layouts/authenticated_header" %>
          <% else %>
            <%= render "layouts/unauthenticated_header" %>
          <% end %>
        </div>
      </div>
    </nav>
    
    <div id="wrap">
      <div class="container">
        <%= yield %>
      </div><!--/.container-->
    </div>

    <%= render "layouts/footer" %>
  </body>
</html>
